{
  "name": "@mexc-sniperbot-ai/api",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "test": "bun test",
    "test:unit": "bun test src/__tests__/unit",
    "test:integration": "bun test src/__tests__/integration",
    "test:performance": "bun test src/__tests__/performance",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch"
  },
  "devDependencies": {
    "@effect/platform": "^0.93.0",
    "@effect/schema": "^0.75.5",
    "effect": "^3.19.2",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@trpc/server": "catalog:",
    "@trpc/client": "catalog:",
    "next": "catalog:",
    "drizzle-orm": "catalog:",
    "ai": "catalog:",
    "@ai-sdk/google": "catalog:",
    "dotenv": "catalog:",
    "zod": "catalog:",
    "@mexc-sniperbot-ai/db": "workspace:*",
    "encore.dev": "^1.50.0",
    "mexc-api-sdk": "^1.0.0",
    "pino": "^9.0.0"
  }
}
